{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:android": {
      "cache": false,
      "persistent": true
    },
    "dev:ios": {
      "cache": false,
      "persistent": true
    },
    "dev:win": {
      "cache": false,
      "persistent": true
    },
    "dev:mac": {
      "cache": false,
      "persistent": true
    },
    "dev:linux": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start:android": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start:ios": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start:win": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start:mac": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start:linux": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
